{
  "name": "html-css-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "swiper": "^8.2.2"
  },
  "devDependencies": {
    "hint": "^6.1.0",
    "prettier": "1.18.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-scss": "^3.18.0"
  },
  "scripts": {
    "format": "npx prettier  --write **/*.{html,json,md,json,yaml,yml,jsx}",
    "format:check": "npx prettier  --check **/*.{html,json,md,json,yaml,yml}",
    "lint": "npx stylelint \"**/*.css\" --fix",
    "lint:check": "npx stylelint \"**/*.css\"",
    "hint": "npx hint --telemetry=off ."
  }
}
